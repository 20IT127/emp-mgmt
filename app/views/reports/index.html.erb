<center>
<h1>Reports</h1>
<table border=5px, align='center'>
<tr><th>user_id</th>
    <th>Title</th>
    <th>Body</th>
    <th>Department</th>
    <th>Created_at</th>
    <th>Updated_at</th>
<tr>
<% @reports.each do |report| %>
<tr>
<td> 
  <%= report.user_id%>
</td>
<td>
  <%= report.title %>
</td>
<td>
  <p><%= report.body %></p>
</td>
<td> 
  <%= report.department %>
</td>
<td>
  <%= report.created_at%>
</td>
<td>
  <%= report.updated_at%>
</td>
</tr>
<% end %>
